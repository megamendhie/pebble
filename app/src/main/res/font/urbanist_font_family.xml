<?xml version="1.0" encoding="utf-8"?>
<font-family xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <!-- regular -->
    <font
        android:font="@font/urbanist_regular"
        android:fontStyle="normal"
        android:fontWeight="500"

        app:font="@font/urbanist_regular"
        app:fontStyle="normal"
        app:fontWeight="500" />

    <!-- italic -->
    <font
        android:font="@font/urbanist_italic"
        android:fontStyle="italic"
        android:fontWeight="500"

        app:font="@font/urbanist_italic"
        app:fontStyle="italic"
        app:fontWeight="500" />

    <!-- bold -->
    <font
        android:font="@font/urbanist_bold"
        android:fontStyle="normal"
        android:fontWeight="700"

        app:font="@font/urbanist_bold"
        app:fontStyle="normal"
        app:fontWeight="700" />
</font-family>